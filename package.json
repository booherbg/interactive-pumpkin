{
  "name": "pumpkin-painter",
  "version": "0.1.0",
  "description": "Interactive Halloween pumpkin LED controller using WLED",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "start:quiet": "WLED_DEBUG=false node src/server.js",
    "start:sim": "node src/server.js --use-simulator",
    "dev": "node --watch --watch-path=./config src/server.js",
    "dev:sim": "node --watch --watch-path=./config src/server.js --use-simulator",
    "test": "node test/test-wled.js"
  },
  "keywords": [
    "wled",
    "led",
    "halloween",
    "pumpkin",
    "lighting"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "express": "^4.18.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

